	var aKeys, i, j, ref, userAgentString;
	userAgentString = navigator.userAgent;
	//- Trident is found in IE 6 - IE 11. Some new versions of IE do not have MSIE string
	aKeys = ["Trident", "Edge", "MSIE", "Opera", "Chrome", "Safari", "Firefox"];
	for (i = j = 0, ref = aKeys.length; (0 <= ref ? j < ref : j > ref); i = 0 <= ref ? ++j : --j) {
		if (userAgentString.indexOf(aKeys[i]) > -1) {
			return aKeys[i];
		}
	}
},
detectVersion = function(browser) {
	return Number(navigator.userAgent.split(`${browser}/`)[1].split('.')[0]);
},

checkBrowserAndNotifyIfUnSupported = function(){
	var version, notSupported;
	supportedBrowsers = ["Chrome", "Safari", "Firefox"];
	thisBrowser = detectBrowser();
	if (supportedBrowsers.indexOf(thisBrowser) == -1) {
		alert("We recommend using Chrome/Firefox/Safari for a better experience.");
		return;
	}
	if (supportedBrowsers.indexOf(thisBrowser) != -1) {
		//- https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Classes#Browser_compatibility
		version = detectVersion(thisBrowser)
		if (thisBrowser == "Chrome"){
			notSupported = (version < 49);
		} else if (thisBrowser == "Safari") {
			notSupported = (version < 9);
		} else if (thisBrowser == "Firefox") {
			notSupported = (version < 45);
		}
		if (notSupported) {
			alert("You are using a very old version of " + thisBrowser + ". Kindly update the browser for using this platform.")
		}
	}
}
checkBrowserAndNotifyIfUnSupported()
